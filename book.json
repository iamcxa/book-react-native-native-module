{
  "title": "React Native 原生模組",
  "author": "Kent Chen <iamcxa@gmail.com>",
  "output.name": "site",
  "language": "zh-tw",
  "gitbook": "3.2.3",
  "root": ".",
  "links": {
    "sidebar": {
      "Trunk-Studio": "http://trunk.studio",
      "Blog": "http://iamcxa.me"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "-highlight",
    "-livereload",
    "search-plus@^0.0.11",
    "simple-page-toc@^0.1.1",
    "github@^2.0.0",
    "github-buttons@2.1.0",
    "edit-link@^2.0.2",
    "disqus@^0.1.0",
    "prism@^2.1.0",
    "prism-themes@^0.0.2",
    "anchors@^0.7.1",
    "include-codeblock@^3.0.2",
    "ace@^0.3.2",
    "katex@^1.1.3",
    "splitter@^0.0.8",
    "mermaid-gb3@2.1.0",
    "tbfed-pagefooter@^0.0.1",
    "expandable-chapters-small@^0.1.7",
    "sectionx@^3.1.0",
    "anchor-navigation-ex@0.1.8",
    "todo@^0.1.3",
    "alerts@^0.2.0",
    "include-csv@^0.1.0",
    "puml@^1.0.1",
    "musicxml@^1.0.2",
    "-sharing",
    "sharing-plus@^0.0.2",
    "graph@^0.1.0",
    "chart@^0.2.0"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": true
    },
    "disqus": {
      "shortName": ""
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
      ]
    },
    "github": {
      "url": "https://github.com/iamcxa/book-react-native-native-module"
    },
    "github-buttons": {
      "repo": "iamcxa/book-react-native-native-module",
      "types": [
        "star"
      ],
      "size": "small"
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "edit": true
    },
    "sharing": {
      "douban": false,
      "facebook": true,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": true,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "facebook", "google", "twitter",
        "weibo", "instapaper", "linkedin",
        "pocket", "stumbleupon", "line", "messenger"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © http://iamcxa.me 2018"
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "edit-link": {
      "base": "https://github.com/iamcxa/book-react-native-native-module/edit/master",
      "label": "Edit This Page"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "sectionx": {
      "tag": "b"
    },
    "favicon": {
      "shortcut": "favicon.ico",
      "bookmark": "favicon.ico"
    }
  }
}